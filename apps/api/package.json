{
  "name": "@openpanel/api",
  "version": "0.0.1",
  "scripts": {
    "dev": "dotenv -e ../../.env -c -v WATCH=1 tsup",
    "testing": "API_PORT=3333 pnpm dev",
    "start": "node dist/index.js",
    "build": "rm -rf dist && tsup",
    "gen:referrers": "jiti scripts/get-referrers.ts && biome format --write src/referrers/index.ts",
