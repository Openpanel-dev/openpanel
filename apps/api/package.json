    "@faker-js/faker": "^9.0.1",
    "@openpanel/sdk": "workspace:*",
    "@openpanel/tsconfig": "workspace:*",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/ramda": "^0.29.6",
    "@types/request-ip": "^0.0.41",
    "@types/source-map-support": "^0.5.10",
    "@types/sqlstring": "^2.3.2",
