{
  "extends": "@openpanel/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
    },
    "plugins": [
      {
        "name": "next",
      },
    ],
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "strictNullChecks": true,
  },
  "include": [".", ".next/types/**/*.ts"],
  "exclude": ["node_modules"],
}
